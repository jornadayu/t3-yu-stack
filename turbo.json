{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "db-generate": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "db-push": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "db-reset": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "db-up": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "db-studio": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "db-nuke": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "db-test-nuke": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "db-test-setup": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "db-setup": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "dev": {
      "dependsOn": ["^db-generate"],
      "cache": false
    },
    "build": {
      "dependsOn": [
        "^build",
        "^db-generate",
        "$NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY",
        "$CLERK_SECRET_KEY"
      ],
      "outputs": [".next/**", ".expo/**"]
    },
    "lint": {
      "outputs": []
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "type-check": {
      "dependsOn": ["^db-generate"],
      "cache": false
    },
    "test": {
      "cache": false
    }
  },
  "globalEnv": [
    "NODE_ENV",
    "PORT",
    "VERCEL_URL",
    "DATABASE_URL",
    "NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY",
    "CLERK_SECRET_KEY"
  ]
}
